(defun average2 (x y)
    (/ (+ x y) 2))
(defun improve2 (guess x)
    (average2 guess (/ x guess)))
(defun abs2 (x)
    (if (< x 0)
    (- x)
    x))
(defun square2 (x)
    (* x x))
(defun good-enough? (guess x)
    (< (abs2 (- (square2 guess) x)) 0.001))
(defun sqrt-iter2 (guess x)
    (if (good-enough? guess x)
    guess
    (sqrt-iter2 (improve2 guess x)
    x)))
(defun sqrt2 (x)
    (sqrt-iter2 1.0 x))
    
(write-line (write-to-string (sqrt2 2)))
